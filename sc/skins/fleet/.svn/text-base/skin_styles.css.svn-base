    border:0;
    padding:0;
    background:#FFFFFF;
}
.textItemDisabled,
.textItemDisabledHint,
.uploadItemDisabled,
.uploadItemDisabledHint {
    color:#ACA899; 
}

.textItemHint,
.uploadItemHint {
    color:darkGray;
}

/* selectItem */

