/* ----event window---- */

.eventWindow {
    font-size:9px;
    background-color:#649ED8;
    border: 1px solid #004D99;
    padding: 0px;
    margin: 0px;
}

.eventWindowHeader,
.eventWindowGripper,
.eventWindowLabel {
    font-size:9px; color: white;
    background-color:#004D99;
    margin: 0px;
}

.eventWindowBody {
    font-size: 9px;
    background-color: #649ED8;
    padding: 3px;
}

.eventWindowResizer{
    border-top:1px solid white;
    border-bottom: 1px solid white;
