/*============================================================
	1) General
============================================================*/

ol,
ul {
    -webkit-text-size-adjust: 100%;
}

/* default text */
.normal,
.normalFocused {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px;
}

/* drag outline */
.dragOutline {
    border:1px dashed #6600FF;
}

.defaultBorder {
    border: 1px solid #606060;
}

/* drop indicator */
.dropBorder {
    background-color:#98A1DA;
}


/* hover canvas  */
.canvasHover,
.gridHover,
.formHover {
    font-family:Verdana,Bitstream Vera Sans,sans-serif; font-size:11px; color:black;
    background-color:#FFFFD0; border:1px solid #808080; padding:5px;
}

/* group label */
.groupLabel {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px;
    padding-left:5px; padding-right:5px;
}
/*============================================================
    2) Button
		button.baseStyle
		buttonStrip.baseStyle
		buttonTable.baseButtonStyle
		listBar.labelStyle
		listGrid.headerBaseStyle (not in this skin)
============================================================*/

/* NOTE: Netscape does not recognize the border attributes in these styles. */
/*		If you change the border attributes here, be sure to update the borderSize, */
/*		tlBorderColor, and brBorderColor defaults in Button.js also. */
.button {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; background-color:#CCCCCC;
    border-left:1px solid #FFFFFF; border-top:1px solid #FFFFFF;
    border-right:1px solid #000000; border-bottom:1px solid #000000;
    padding:2px;        
}
.buttonOver {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; background-color:#DCDCDC;
    border-left:1px solid #FFFFFF; border-top:1px solid #FFFFFF;
    border-right:1px solid #000000; border-bottom:1px solid #000000;
    padding:2px;        
}
.buttonDown {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; background-color:#888888; 
    border-left:1px solid #000000; border-top:1px solid #000000;
    border-right:1px solid #FFFFFF; border-bottom:1px solid #FFFFFF;
    padding:2px;        
}
.buttonDisabled {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; color:#666666;
    background-color:#CCCCCC;
    border-left:1px solid #FFFFFF; border-top:1px solid #FFFFFF;
    border-right:1px solid #000000; border-bottom:1px solid #000000;
    padding:2px;        
}
.buttonSelected,
.buttonSelectedDown,
.buttonSelectedOver {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; background-color:#888888; 
    border-left:1px solid #000000; border-top:1px solid #000000;
    border-right:1px solid #000000; border-bottom:1px solid #000000;
    padding:2px;        
}
.buttonSelectedDisabled {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; color:#666666;
    background-color:#CCCCCC;
    border-left:1px solid #FFFFFF; border-top:1px solid #FFFFFF;
    border-right:1px solid #000000; border-bottom:1px solid #000000;
    padding:2px;        
}
                
/* ImgButton */
.imgButton,
.imgButtonOver,
.imgButtonSelected,
.imgButtonSelectedDisabled,
.imgButtonSelectedOver {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px;
}
.imgButtonDown,
.imgButtonSelectedDown {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px;
}
.imgButtonDisabled {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; color:#666666;
}

/* StretchImgButton */
.stretchImgButton,
.stretchImgButtonOver,
.stretchImgButtonSelected,
.stretchImgButtonSelectedOver {
    font-family:Verdana,Bitstream Vera Sans,sans-serif; font-size:10px;
}
.stretchImgButtonDown,
.stretchImgButtonSelectedDown {
    font-family:Verdana,Bitstream Vera Sans,sans-serif; font-size:10px;
}
.stretchImgButtonSelectedDisabled,
.stretchImgButtonDisabled {
    font-family:Verdana,Bitstream Vera Sans,sans-serif; font-size:10px; color:#666666;
}



/*============================================================
    3) Menus 
============================================================*/

.menuBorder {
    border-top:2px outset white; border-bottom:2px outset white;
    border-left:2px outset white; border-right:2px outset white;
}
.menu,
.menuSelected {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; font-weight:bold;
    background-color:#CCCCCC;
    border-top:1px solid #CCCCCC;
    border-bottom:1px solid #CCCCCC;
}
.menuOver,
.menuSelectedOver {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; color:white; font-weight:bold;
    background-color:black;
    border-top:1px solid lightBlue;
    border-bottom:1px solid darkBlue;
}
.menuDisabled {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; color:#999999; font-weight:bold;
    background-color:#CCCCCC;
    border-top:1px solid #CCCCCC;
    border-bottom:1px solid #CCCCCC;
}

/* menu header buttons */
.menuButtonText {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; font-weight:bold;    
}
.menuButton,
.menuButtonOver {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; background-color:#CCCCCC;
    border-left:1px solid #FFFFFF; border-top:1px solid #FFFFFF;
    border-right:1px solid #000000; border-bottom:1px solid #000000;
    padding-left:4px; padding-right:4px;    
}
.menuButtonDown {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; background-color:#888888; 
    border-left:1px solid #000000; border-top:1px solid #000000;
    border-right:1px solid #FFFFFF; border-bottom:1px solid #FFFFFF;
    padding-left:4px; padding-right:4px;    
}
.menuButtonDisabled {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; color:#666666;
    background-color:#CCCCCC;
    border-left:1px solid #FFFFFF; border-top:1px solid #FFFFFF;
    border-right:1px solid #000000; border-bottom:1px solid #000000;
    padding-left:4px; padding-right:4px;        
}
.menuButtonSelected,
.menuButtonSelectedDown,
.menuButtonSelectedOver {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; background-color:#888888; 
    border-left:1px solid #000000; border-top:1px solid #000000;
    border-right:1px solid #000000; border-bottom:1px solid #000000;
    padding-left:4px; padding-right:4px;    
}
.menuButtonSelectedDisabled {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; color:#666666;
    background-color:#CCCCCC;
    border-left:1px solid #FFFFFF; border-top:1px solid #FFFFFF;
    border-right:1px solid #000000; border-bottom:1px solid #000000;
    padding-left:4px; padding-right:4px;    
}
.treeMenuSelected {
    font-family:Verdana,Bitstream Vera Sans,sans-serif; font-size:11px; color:black; background-color:#A9B2CA; 
    border-top:1px solid #A9B2CA;
    border-bottom:1px solid #A9B2CA;
}
.treeMenuSelectedOver {
    font-family:Verdana,Bitstream Vera Sans,sans-serif; font-size:11px; color:white; background-color:#0A246A;
    border-top:1px solid #0A246A;
    border-bottom:1px solid #0A246A;
}



/*============================================================
    4) Window
		window.styleName - windowBackground
		window.bodyStyle - windowBody
		window.headerStyle - windowHeader
		window.hiliteHeaderStyle - windowHeaderHilite
		window.headerLabelDefaults.styleName - windowHeaderText
		window.statusBarDefaults.styleName - windowStatusBar

	Dialog
		dialog.styleName - dialogBackground
		dialog.bodyStyle - dialogBody
		dialog.headerStyle - dialogHeader
		dialog.hiliteHeaderStyle - dialogHeaderHilite
		dialog.headerLabelDefaults.styleName - dialogHeaderText
============================================================*/

/* Window */
.windowBackground {
    border-top:1px solid black; border-bottom:1px solid black; 
    border-left:1px solid black; border-right:1px solid black;
}
.windowBody {
    border-top:2px inset #666666; border-bottom:2px inset #666666;
    border-left:2px inset #666666; border-right:2px inset #666666;
}
.windowHeader {}
.windowHeaderHilite {}
.windowHeaderText {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; 
    color:black; font-weight:bold;
    background-color:#DEDEDE; 
    padding-top:1px;
    padding-left:4px; padding-right:4px;
}
.windowStatusBar {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px;
    border: 1px solid black;
}
.windowShadow {
    background-color:#AAAAAA;
    filter: alpha(opacity=35, style=0);
    -moz-opacity: 10%;
}
.modalMask {
    background-color:black;
}

/* Dialog */
.dialogBackground {
    border-top:1px solid black; border-bottom:1px solid black; 
    border-left:1px solid black; border-right:1px solid black;
}
.dialogBody,
.promptBody {}
.dialogHeader {}
.dialogHeaderHilite {}
.dialogHeaderText {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; 
    color:black; font-weight:bold;
    background-color:#DEDEDE;
    padding-left:4px; padding-right:4px;
}
.dialogStatusBar {
    border-left: 1px solid black; border-right: 1px solid black; 
    border-bottom: 1px solid black;
}
.dialogStatusBarText {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px;
}
.dialogShadow {
    background-color:#AAAAAA;
    filter: alpha(opacity=35, style=0);
    -moz-opacity: 10%;
}

/*============================================================
    5) Layout
============================================================*/

.layoutDropLine {
    background-color:#DDDDFF;
}
.layoutPlaceHolder {
    border:1px solid black;
}

/*============================================================
    6) TabSet and tab titles
============================================================*/

.tabSetContainer {
    background-color: #EEEEEE;
    border-left:1px solid black; border-top:1px solid black;
    border-right:1px solid black; border-bottom:1px solid black;
}
.tab,
.tabOver,
.tabSelected,
.tabSelectedOver {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; font-weight:bold;
}
.tabDown,
.tabSelectedDown {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; font-weight:bold;
}
.tabSelectedDisabled,
.tabDisabled {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; color:#666666; font-weight:bold;
}



/*============================================================
    7) SectionStack
============================================================*/

/* SectionStack headers - css based */
.sectionHeaderopened, 
.sectionHeaderclosed {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; background-color:#CCCCCC;
    border-left:1px solid #FFFFFF; border-top:1px solid #FFFFFF;
    border-right:1px solid #000000; border-bottom:1px solid #000000;
}
.sectionHeaderDisabledopened,
.sectionHeaderDisabledclosed	 {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; color:#666666;
    background-color:#CCCCCC;
    border-left:1px solid #FFFFFF; border-top:1px solid #FFFFFF;
    border-right:1px solid #000000; border-bottom:1px solid #000000;
}



/*============================================================
    8) Forms
============================================================*/

/* general form styles */
.formRow {}
.formCell,
.formCellFocused,
.formCellDisabled {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px;
}
.formTitle,
.formTitleFocused,
.formTitleDisabled {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px;
}
.formCellError,
.formTitleError {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; font-weight:bold; color:darkred;
    background-color:#FAFAD2
}
.formHint,
.formHintDisabled {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; color:#666666; padding:3px;
}

/* headerItem */
.headerItem,
.headerItemDisabled,
.headerItemError {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px; font-weight:bold; color:darkblue;
}

/* text items */
.nativeSelectItem,
.nativeSelectItemDisabled,
.nativeSelectItemError,
.staticTextItem,
.staticTextItemDisabled,
.staticTextItemError {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px;
}

.textItem,
.textItemFocused,
.textItemDisabled,
.textItemDisabledHint,
.textItemError,
.textItemHint {
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px;
    padding:1px; border:2px inset #D4D0C8;
    background-color:#FFFFFF;
}
.uploadItem,
.uploadItemFocused,
.uploadItemDisabled,
.uploadItemDisabledHint,
.uploadItemError,
.uploadItemHint {
    margin:0;
    border:0;
    padding:0;
    font-family:Bitstream Vera Sans,Helvetica,Verdana,sans-serif; font-size:11px;
    background:#FFFFFF;
}

/*
Cupertino relies on the browser's user-agent stylesheet to apply a disabled text color to
disabled TextItems. The GrayText CSS2 system color keyword should refer to this disabled text
color, but some browsers (e.g. Firefox 31.0, Chrome 36) actually use a different color. So,
we apply color:GrayText ourselves.
*/
.textItemDisabled,
.textItemDisabledHint,
.uploadItemDisabled,
.uploadItemDisabledHint {
    color: GrayText;
}

.textItemHint,
.uploadItemHint {
    color:darkGray;
}

/* selectItem */

.selectItemText,
.selectItemTextError,
.selectItemTextItemHint {
    font-family:Verdana,Bitstream Vera Sans,Helvetica,sans-serif; font-size:11px; margin:2px;
}
.selectItemTextFocused {
    font-family:Verdana,Bitstream Vera Sans,Helvetica,sans-serif; font-size:11px; margin:2px;
    background-color:#316AC5; color:white;
}

.selectItemTextDisabled,
.selectItemTextDisabledHint {
    font-family:Verdana,Bitstream Vera Sans,Helvetica,sans-serif; font-size:11px; margin:2px;
    color:GrayText;
}

.selectItemTextItemHint {
    color:#666666;
}

.selectItemControl,
.selectItemControlError,
.selectItemControlFocused {
    background-color:white; border:1px solid #808080;
}

.selectItemControlDisabled {
    background-color:white; border:1px solid #A0A0A0;
}

.selectItemPickerIcon,
.selectItemPickerIconFocused,
.selectItemPickerIconDisabled,
.selectItemPickerIconError {
    padding:1px;
}

/*
Cupertino relies on the browser's user-agent stylesheet to apply a disabled text color to
disabled TextItems. Use the CSS2 system color keyword 'GrayText'.
*/
.nativeSelectItemDisabled,
.staticTextItemDisabled {
    color: GrayText;
}

